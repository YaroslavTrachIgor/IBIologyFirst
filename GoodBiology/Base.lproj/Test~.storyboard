                                                <textView clipsSubviews="YES" multipleTouchEnabled="YES" contentMode="scaleToFill" fixedFrame="YES" textAlignment="center" translatesAutoresizingMaskIntoConstraints="NO" id="Akd-4j-cZi">
                                                    <rect key="frame" x="17" y="43" width="341" height="651"/>
